{
  "name": "dom2txml",
  "version": "1.0",
  "description": "Mapping DOM path to TXML path",
  "manifest_version": 2,
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "icons": {
    "128": "images/logo-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/logo-32.png"
    }
  },
  "permissions": ["activeTab", "storage", "downloads", "declarativeContent"],
  "web_accessible_resources": [
    "content-min.js",
    "content/content.css",
    "content/content.html",
    "images/*"
  ]
}
